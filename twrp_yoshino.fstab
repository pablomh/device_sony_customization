# mount point   fstype  device                                      device2                 fstype2
/boot           emmc    /dev/block/bootdevice/by-name/boot
/recovery       emmc    /dev/block/bootdevice/by-name/FOTAKernel
/system         ext4    /dev/block/bootdevice/by-name/system                                flags=mounttodecrypt
/cache          ext4    /dev/block/bootdevice/by-name/cache
/data           ext4    /dev/block/bootdevice/by-name/userdata                              length=-16384
/odm            ext4    /dev/block/bootdevice/by-name/oem                                   flags=display="OEM";mounttodecrypt;flashimg;backup;backupname="OEM"

/external_sd    auto    /dev/block/mmcblk0p1                        /dev/block/mmcblk0      flags=display="Micro SDcard";storage;wipeingui;removable
